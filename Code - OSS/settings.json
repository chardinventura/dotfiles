{
    "html.autoClosingTags": true,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "java.configuration.updateBuildConfiguration": "automatic",
    "java.signatureHelp.enabled": true,
    "vim.easymotion": true,
    "vim.leader": " ",
    "vim.searchHighlightColor": "#F44",
    "vim.smartRelativeLine": true,
    "vim.statusBarColorControl": true,
    "workbench.colorCustomizations": {
        "[Mayukai Dark]": {
            "statusBar.foreground": "#ffffff",
            "statusBar.background": "#000000",
            "activityBar.foreground": "#ffffff",
            "tab.activeForeground": "#fbff00",
            "tab.inactiveForeground": "#ffffff"
        },
        "[Monokai]": {
            "editor.lineHighlightBackground": "#000000"
        },
        "statusBar.background": "#005f5f",
        "statusBar.noFolderBackground": "#005f5f",
        "statusBar.debuggingBackground": "#005f5f"
    },
    "editor.tokenColorCustomizations": {
        "[Monokai]": {
            "textMateRules": [
                {
                    "scope": "entity.name.type",
                    "settings": {
                        "fontStyle": "bold"
                    }
                },
                {
                    "scope": "entity.name.namespace",
                    "settings": {
                        "fontStyle": ""
                    }
                }
            ]
        }
    },
    "java.project.importOnFirstTimeStartup": "automatic",
    "java.refactor.renameFromFileExplorer": "autoApply",
    "vim.highlightedyank.enable": true,
    "vim.useSystemClipboard": true,
    "vim.camelCaseMotion.enable": true,
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "<leader>",
                "a",
                "c"
            ],
            "commands": [
                "editor.action.sourceAction"
            ]
        },
        {
            "before": [
                "<leader>",
                "n"
            ],
            "commands": [
                ":bnext"
            ]
        },
        {
            "before": [
                "<leader>",
                "p"
            ],
            "commands": [
                ":bprevious"
            ]
        },
        {
            "before": [
                "<leader>",
                "<enter>"
            ],
            "commands": [
                "editor.action.formatDocument"
            ]
        },
        {
            "before": [
                "<leader>",
                "r",
                "n"
            ],
            "commands": [
                "editor.action.rename"
            ]
        },
        {
            "before": [
                "g",
                "D"
            ],
            "commands": [
                "editor.action.goToTypeDefinition"
            ]
        },
        {
            "before": [
                "g",
                "i"
            ],
            "commands": [
                "editor.action.goToImplementation"
            ]
        },
        {
            "before": [
                "g",
                "r"
            ],
            "commands": [
                "editor.action.goToReferences"
            ]
        },
        {
            "before": [
                "<leader>",
                "r",
                "u"
            ],
            "commands": [
                "workbench.action.debug.start"
            ]
        },
        {
            "before": [
                "<leader>",
                "r",
                "r"
            ],
            "commands": [
                "workbench.action.debug.restart"
            ]
        },
        {
            "before": [
                "<leader>",
                "s"
            ],
            "commands": [
                "workbench.action.debug.stop"
            ]
        },
        {
            "before": [
                "<leader>",
                "<tab>"
            ],
            "commands": [
                "workbench.view.explorer"
            ]
        },
        {
            "before": [
                "<leader>",
                "o",
                "i"
            ],
            "commands": [
                "editor.action.organizeImports"
            ]
        },
        {
            "before": [
                "<leader>",
                "q",
                "f"
            ],
            "commands": [
                "editor.action.quickFix"
            ]
        }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": [
                "<leader>",
                "a"
            ],
            "commands": [
                "editor.action.sourceAction"
            ]
        }
    ],
    "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx1G -Xms100m -javaagent:\"/home/chardinventura/.vscode-oss/extensions/gabrielbb.vscode-lombok-1.0.0/server/lombok.jar\"",
    "editor.fontFamily": "'JuliaMono'",
    "editor.renderWhitespace": "trailing",
    "editor.fontLigatures": true,
    "editor.inlineHints.fontFamily": "'JuliaMono'",
    "debug.console.fontFamily": "JuliaMono",
    "workbench.iconTheme": "material-icon-theme",
    "files.autoSaveDelay": 1,
    "emmet.triggerExpansionOnTab": true,
    "vim.useCtrlKeys": true,
    "workbench.colorTheme": "Monokai",
    "editor.minimap.enabled": false,
    "window.zoomLevel": -1,
    "[yaml]": {
        "editor.insertSpaces": false,
        "editor.tabSize": 2,
        "editor.autoIndent": "advanced"
    },
    "spring.initializr.defaultGroupId": "com.nardich",
    "spring.initializr.defaultJavaVersion": "11",
    "spring.initializr.defaultLanguage": "Java",
    "spring.initializr.defaultOpenProjectMethod": "Open",
    "spring.initializr.defaultPackaging": "JAR"
}